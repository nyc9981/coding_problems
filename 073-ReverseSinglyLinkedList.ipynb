   "source": [
    "# assume head is singly linked list: node, next\n",
    "def reverse_singly_linked_list(head):\n",
    "    if head: # not empty \n",
    "        curr = head.next\n",
    "        while curr:\n",
    "            temp = curr.next\n",
    "            curr.next = head\n",
    "            head = curr\n",
    "            curr = temp"
   ]
  }
