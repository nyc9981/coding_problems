   "source": [
    "# return x if b=1, y if b=0\n",
    "def switch(x, y, b):\n",
    "    return x*b + y*(1-b)\n",
    "\n",
    "def switch2(x, y, b):\n",
    "    return x*b | y*(1-b)\n",
    "\n",
    "def switch3(x, y, b):\n",
    "    return x**b * y**(1-b)"
   ]
  },
